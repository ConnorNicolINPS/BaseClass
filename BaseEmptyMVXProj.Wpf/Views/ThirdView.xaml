<views:MvxWpfView x:Class="BaseEmptyMVXProj.Wpf.Views.ThirdView"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                  xmlns:views="clr-namespace:MvvmCross.Wpf.Views;assembly=MvvmCross.Wpf"
                  mc:Ignorable="d" >

    <views:MvxWpfView.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Resources/ButtonStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </views:MvxWpfView.Resources>

    <StackPanel Background="LightGreen">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" HorizontalAlignment="Center">
            <Button Content="&lt;&lt; Go to First View" Background="Green" Command="{Binding GoToFirstViewCommand}"/>
            <Button Content="&lt; Go to Second View" Background="Green" Command="{Binding GoToSecondViewCommand}"/>
        </StackPanel>

        <Button Content="Close" HorizontalAlignment="Left" Background="Red" Command="{Binding CloseCommand}"/>
    </StackPanel>
</views:MvxWpfView>
